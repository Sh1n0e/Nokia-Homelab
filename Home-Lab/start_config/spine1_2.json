    "srl_nokia-ssh:ssh-server": [
      {
        "name": "default",
        "admin-state": "enable",
        "network-instance": "default"
      },
      {
        "name": "mgmt",
        "admin-state": "enable",
        "network-instance": "mgmt",
        "use-credentialz": true
      }
    ],
    "srl_nokia-lldp:lldp": {
      "admin-state": "enable",
      "hello-timer": "30",
      "management-address": [
        {
          "subinterface": "mgmt0.0",
          "type": [
            "IPv4"
          ]
        }
      ],
      "interface": [
        {
          "name": "mgmt0",
          "admin-state": "disable"
        }
      ]
    },
