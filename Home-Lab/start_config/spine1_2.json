    "srl_nokia-grpc:grpc-server": [
      {
        "name": "mgmt",
        "admin-state": "enable",
        "timeout": 7200,
        "rate-limit": 60,
        "session-limit": 20,
        "tls-profile": "tls-profile-1",
        "network-instance": "mgmt",
        "port": 50052,
        "services": [
          "srl_nokia-grpc:gnmi",
          "srl_nokia-grpc:gnoi"
        ]
      }
    ],
    "srl_nokia-json-rpc:json-rpc-server": {
      "admin-state": "enable",
      "network-instance": [
        {
          "name": "mgmt",
          "http": {
            "admin-state": "enable",
            "use-authentication": true,
            "session-limit": 1,
            "port": 4000
          },
          "https": {
            "admin-state": "enable",
            "use-authentication": true,
            "session-limit": 1,
            "port": 443,
            "tls-profile": "tls-profile-1"
          }
        }
      ]
    }
  },
  "srl_nokia-routing-policy:routing-policy": {
    "policy": [
      {
        "name": "bgp-export-policy",
        "statement": [
          {
            "name": "999",
            "action": {
              "policy-result": "accept"
            }
          }
        ]
      }
    ]
  }
