{
  "srl_nokia-network-instance:network-instance": [
    {
      "name": "default",
      "type": "srl_nokia-network-instance:default",
      "admin-state": "enable",
      "interface": [
        {
          "name": "ethernet-1/1.0"
        },
        {
          "name": "ethernet-1/2.0"
        },
        {
          "name": "lo0.0"
        }
      ],
      "protocols": {
        "srl_nokia-linux:linux": {
          "export-routes": true,
          "export-neighbors": true
        },
        "srl_nokia-bgp:bgp": {
          "autonomous-system": 64999,
          "router-id": "100.0.1.1",
          "ebgp-default-policy": {
            "import-reject-all": false,
            "export-reject-all": true
          },
          "afi-safi": [
            {
              "afi-safi-name": "srl_nokia-common:ipv4-unicast",
              "admin-state": "enable",
              "multipath": {
                "allow-multiple-as": true,
                "ebgp": {
                  "maximum-paths": 2
                },
                "ibgp": {
                  "maximum-paths": 2
                }
              }
            }
          ],
          "group": [
            {
              "group-name": "leaf",
              "admin-state": "enable"
            }
          ],
          "neighbor": [
            {
              "peer-address": "10.1.1.1",
              "admin-state": "enable",
              "description": "default network-instance bgp neighbor to Leaf-1",
              "peer-as": 65001,
              "peer-group": "leaf",
              "export-policy": [
                "bgp-export-policy"
              ]
            },
            {
              "peer-address": "10.1.2.1",
              "description": "default network-instance bgp neighbor to Leaf-2",
              "peer-as": 65002,
              "peer-group": "leaf",
              "export-policy": [
                "bgp-export-policy"
              ]
            }
          ]
        }
      }
    },
    {
      "name": "mgmt",
      "type": "srl_nokia-network-instance:ip-vrf",
      "admin-state": "enable",
      "description": "Management network instance",
      "interface": [
        {
          "name": "mgmt0.0"
        }
      ],
      "protocols": {
        "srl_nokia-linux:linux": {
          "export-routes": true,
          "export-neighbors": true
        }
      }
    }
  ],
  "srl_nokia-interfaces:interface": [
    {
      "name": "ethernet-1/1",
      "admin-state": "enable",
      "subinterface": [
        {
          "index": 0,
          "admin-state": "enable",
          "ipv4": {
            "admin-state": "enable",
            "address": [
              {
                "ip-prefix": "10.1.1.0/31"
              }
            ]
          }
        }
      ]
    },
    {
      "name": "ethernet-1/2",
      "admin-state": "enable",
      "subinterface": [
        {
          "index": 0,
          "admin-state": "enable",
          "ipv4": {
            "admin-state": "enable",
            "address": [
              {
                "ip-prefix": "10.1.2.0/31"
              }
            ]
          }
        }
      ]
    },
    {
      "name": "lo0",
      "admin-state": "enable",
      "subinterface": [
        {
          "index": 0,
          "admin-state": "enable",
          "ipv4": {
            "admin-state": "enable",
            "address": [
              {
                "ip-prefix": "100.0.1.1/32"
              }
            ]
          },
          "ipv6": {
            "admin-state": "enable",
            "address": [
              {
                "ip-prefix": "2001::100:0:1:1/128"
              }
            ]
          }
        }
      ]
    },
    {
      "name": "mgmt0",
      "admin-state": "enable",
      "mtu": 1500,
      "subinterface": [
        {
          "index": 0,
          "admin-state": "enable",
          "ipv4": {
            "admin-state": "enable",
            "srl_nokia-interfaces-ip-dhcp:dhcp-client": {
              
            }
          },
          "ipv6": {
            "admin-state": "enable",
            "srl_nokia-interfaces-ip-dhcp:dhcp-client": {
              
            }
          }
        }
      ]
    }
  ],
